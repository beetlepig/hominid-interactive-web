		"@theatre/core": "^0.7.2",
		"@theatre/studio": "^0.7.2",
		"@threlte/core": "^8.1.5",
		"@threlte/extras": "^9.5.2",
		"@threlte/theatre": "^3.0.7",
		"bits-ui": "^2.9.4",
