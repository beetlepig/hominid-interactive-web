		"globals": "^16.0.0",
		"prettier": "^3.8.1",
		"prettier-plugin-jsdoc": "^1.5.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.51.2",
		"svelte-check": "^4.4.0",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
