		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/kit": "^2.52.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.17",
		"@trivago/prettier-plugin-sort-imports": "^6.0.2",
		"@types/node": "^24.1.0",
		"@types/three": "^0.182.0",
