		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"@threlte/theatre": "^3.1.2",
		"bits-ui": "^2.15.5",
		"motion": "^12.34.0",
		"three": "^0.182.0"
